type User {
  id: ID! @unique
  name: String!
  email: String! @unique
  password: String!
  createdAt: DateTime!
  updatedAt: DateTime!
}

type Product {
  id: ID! @unique
  department: String!
  title: String!
  description: String!
  image: String!
  category: String
  brand: String
  status: String @default(value: "Out of Stock")
  url: String
  user: User!
  createdAt: DateTime!
  updatedAt: DateTime!
}
